{
  "manifest_version": 2,

  "name": "My Extension",
  "description": "This extension is a starting point to create a real Chrome extension",
  "version": "0.0.1",

  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "icons": {
    "16": "logo192.png",
    "48": "logo512.png"
  },
  "permissions": [
    "activeTab"
    ],
  "content_security_policy": "script-src 'self' 'sha256-Isw/ysxK2VNKTGP4kOShm8PB+E9fRCoGcDh0xk1unj4='; object-src 'self'"
}